#####RESTRICTIONS

#Every work must have at lest one movement
mo:MusicalWork rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:hasMovement ; owl:someValuesFrom mso:Movement ] ,
  [ a owl:Restriction ; owl:onProperty so:hasKey ; owl:allValuesFrom mto:Key ] .

#Every mvt must have at lest one sctn
mso:Movement rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:hasSection ; owl:someValuesFrom mno:Section ] ,
  [ a owl:Restriction ; owl:onProperty so:hasSection ; owl:allValuesFrom mno:Section ] .

#Every sctn must have at lest one phrase
mno:Section rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:hasPhrase ; owl:someValuesFrom so:Phrase ] ,
  [ a owl:Restriction ; owl:onProperty so:hasPhrase ; owl:allValuesFrom so:Phrase ] .

#Every phrase has star and end measure, and at least one measure
so:Phrase rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:hasMeasure ;   owl:someValuesFrom mso:Measure ] ,
  [ a owl:Restriction ; owl:onProperty so:startMeasure ; owl:someValuesFrom mso:Measure ] ,
  [ a owl:Restriction ; owl:onProperty so:endMeasure ;   owl:someValuesFrom mso:Measure ] .

# Every measure must have a time signature; if it hasTimeSignature
mso:Measure rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:number ; owl:minCardinality "1"^^xsd:nonNegativeInteger ] ,
  [ a owl:Restriction ; owl:onProperty so:hasTimeSignature ; owl:allValuesFrom mso:TimeSignature ] .

#Note and Rest have the same ObjectProperty of SymbolicEvent
ho:SymbolicEvent rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:measure ;     owl:cardinality "1"^^xsd:nonNegativeInteger ] ,
  [ a owl:Restriction ; owl:onProperty so:hasDuration ; owl:someValuesFrom so:Duration ] .

mso:Note rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:hasPitch ; owl:someValuesFrom so:Pitch ] ,
  [ a owl:Restriction ; owl:onProperty so:hasPitch ; owl:allValuesFrom  so:Pitch ] .


so:Pitch rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:hasAccidental ; owl:allValuesFrom so:Accidental ] .

co:Chord rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:chordMeasure ;   owl:allValuesFrom mso:Measure ] ,
  [ a owl:Restriction ; owl:onProperty so:hasRomanNumeral ; owl:allValuesFrom so:RomanNumeral ] .

co:Chord rdfs:subClassOf
  [ a owl:Restriction ;
    owl:onProperty              so:hasNoteInChord ;
    owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
    owl:onClass                 mso:Note
  ] .


so:HarmonicProgression rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:hasChord ;
    owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
    owl:onClass co:Chord
  ] .

so:Tempo rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:tempoOf ; owl:allValuesFrom mso:Measure ] .

so:KeySignature rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty so:isKeySignatureOfMeasure ; owl:allValuesFrom mso:Measure ] .


### Disjointness 

[ a owl:AllDisjointClasses ;
  owl:members ( so:Exposition so:Development so:Recapitulation )
] .

[ a owl:AllDisjointClasses ;
  owl:members ( so:ppp so:pp so:p so:mp so:mf so:f so:ff so:fff )
] .

[ a owl:AllDisjointClasses ;
  owl:members ( so:Staccato so:Tenuto so:Accent so:Legato )
] .

[ a owl:AllDisjointClasses ;
  owl:members ( so:A so:B so:C so:D so:E so:F so:G )
] .

[ a owl:AllDisjointClasses ;
  owl:members ( so:WholeNote so:HalfNote so:QuarterNote so:EighthNote
                so:SixteenthNote so:ThirtySecondNote so:SixtyFourthNote
                so:DottedHalf so:DottedQuarter so:DottedEighth )
] .

### ----- PHRASE / SECTION -----

# Section → Measure
so:sectionHasMeasure a owl:ObjectProperty ;
  rdfs:domain mno:Section ; rdfs:range mso:Measure ;
  rdfs:label "section has measure"@en ;
  owl:propertyChainAxiom ( so:hasPhrase so:hasMeasure ) .

# Phrase → SymbolicEvent
so:phraseHasSymbolicEvent a owl:ObjectProperty ;
  rdfs:domain so:Phrase ; rdfs:range ho:SymbolicEvent ;
  rdfs:label "phrase has symbolic event"@en ;
  owl:propertyChainAxiom ( so:hasMeasure so:hasSymbolicEvent ) .

# Section → SymbolicEvent
so:sectionHasSymbolicEvent a owl:ObjectProperty ;
  rdfs:domain mno:Section ; rdfs:range ho:SymbolicEvent ;
  rdfs:label "section has symbolic event"@en ;
  owl:propertyChainAxiom ( so:hasPhrase so:hasMeasure so:hasSymbolicEvent ) .

### ----- MOVEMENT -----

# Movement → Measure
so:movementHasMeasure a owl:ObjectProperty ;
  rdfs:domain mso:Movement ; rdfs:range mso:Measure ;
  rdfs:label "movement has measure"@en ;
  owl:propertyChainAxiom ( so:hasSection so:hasPhrase so:hasMeasure ) .

# Movement → SymbolicEvent
so:movementHasSymbolicEvent a owl:ObjectProperty ;
  rdfs:domain mso:Movement ; rdfs:range ho:SymbolicEvent ;
  rdfs:label "movement has symbolic event"@en ;
  owl:propertyChainAxiom ( so:hasSection so:hasPhrase so:hasMeasure so:hasSymbolicEvent ) .

# Movement → Chord
so:movementHasChord a owl:ObjectProperty ;
  rdfs:domain mso:Movement ; rdfs:range co:Chord ;
  rdfs:label "movement has chord"@en ;
  owl:propertyChainAxiom ( so:hasSection so:hasPhrase so:hasMeasure so:measureHasChord ) .

### ----- WORK (obra) -----

# Work → Measure
so:workHasMeasure a owl:ObjectProperty ;
  rdfs:domain mo:MusicalWork ; rdfs:range mso:Measure ;
  rdfs:label "work has measure"@en ;
  owl:propertyChainAxiom ( so:hasMovement so:movementHasMeasure ) .

# Work → SymbolicEvent
so:workHasSymbolicEvent a owl:ObjectProperty ;
  rdfs:domain mo:MusicalWork ; rdfs:range ho:SymbolicEvent ;
  rdfs:label "work has symbolic event"@en ;
  owl:propertyChainAxiom ( so:workHasMeasure so:hasSymbolicEvent ) .

# Section → Chord
so:sectionHasChord a owl:ObjectProperty ;
  rdfs:domain mno:Section ; rdfs:range co:Chord ;
  rdfs:label "section has chord"@en ;
  owl:propertyChainAxiom ( so:hasPhrase so:hasMeasure so:measureHasChord ) .

# Work → Chord
so:workHasChord a owl:ObjectProperty ;
  rdfs:domain mo:MusicalWork ; rdfs:range co:Chord ;
  rdfs:label "work has chord"@en ;
  owl:propertyChainAxiom ( so:workHasMeasure so:measureHasChord ) .