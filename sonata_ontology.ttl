@prefix : <https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#> .
@prefix co: <http://purl.org/ontology/chord/> .
@prefix ho: <https://github.com/andreamust/HaMSE_Ontology/schema#> .
@prefix mo: <http://purl.org/ontology/mo/> .
@prefix so: <https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix mno: <https://purl.org/andreapoltronieri/notepattern#> .
@prefix mso: <http://linkeddata.uni-muenster.de/ontology/musicscore#> .
@prefix mto: <http://purl.org/ontology/mto/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@base <https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#> .

<https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#> rdf:type owl:Ontology ;
                                                                      dct:creator "Sharon Valdivia" ;
                                                                      dct:description "An ontology focused on the analysis, representation, and preservation of piano sonatas."@en ;
                                                                      dct:title "Sonata Ontology"@en ;
                                                                      vann:preferredNamespacePrefix "so" ;
                                                                      vann:preferredNamespaceUri "https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#" ;
                                                                      owl:versionInfo "0.1.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#maxQualifiedCardinality
owl:maxQualifiedCardinality rdf:type owl:AnnotationProperty .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#candidateKey
so:candidateKey rdf:type owl:AnnotationProperty ;
                rdfs:comment "Schema-level mapping: major/minor key classes that a given key-signature class typically represents (e.g., C major and A minor for KS_0)."@en ;
                rdfs:label "candidate key"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#relativeMajor
so:relativeMajor rdf:type owl:AnnotationProperty .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#relativeMinor
so:relativeMinor rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#chordMeasure
so:chordMeasure rdf:type owl:ObjectProperty ;
                owl:inverseOf so:hasChordInMeasure ;
                rdfs:domain co:Chord ;
                rdfs:range mso:Measure ;
                rdfs:comment "Measure in which the chord occurs (anchoring the chord in the score)."@en ;
                rdfs:label "occurs in measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#endMeasure
so:endMeasure rdf:type owl:ObjectProperty ;
              rdfs:domain so:StructuralElement ;
              rdfs:range mso:Measure ;
              rdfs:comment "Last measure covered by the structural element (movement, section, phrase)."@en ;
              rdfs:label "end measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasAccidental
so:hasAccidental rdf:type owl:ObjectProperty ;
                 rdfs:domain so:Pitch ;
                 rdfs:range mto:Accidental ;
                 rdfs:comment "Associates a pitch with an accidental (sharp, flat, natural, etc.)."@en ;
                 rdfs:label "has accidental"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasArticulation
so:hasArticulation rdf:type owl:ObjectProperty ;
                   owl:inverseOf so:isArticulationOf ;
                   rdfs:domain ho:SymbolicEvent ;
                   rdfs:range mso:Articulation ;
                   rdfs:comment "Articulation marking attached to the symbolic event (e.g., staccato, accent)."@en ;
                   rdfs:label "has articulation"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasChord
so:hasChord rdf:type owl:ObjectProperty ;
            rdfs:domain so:HarmonicProgression ;
            rdfs:range co:Chord ;
            rdfs:comment "Chord belonging to the harmonic progression."@en ;
            rdfs:label "has chord"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasChordInMeasure
so:hasChordInMeasure rdf:type owl:ObjectProperty ;
                     rdfs:domain mso:Measure ;
                     rdfs:range co:Chord ;
                     rdfs:comment "Associates a measure with the chords that occur in it."@en ;
                     rdfs:label "has chord in measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasClef
so:hasClef rdf:type owl:ObjectProperty ;
           rdfs:domain ho:SymbolicEvent ;
           rdfs:range mso:Clef ;
           rdfs:comment "Associates the symbolic event with the clef in effect."@en ;
           rdfs:label "has clef"@en .

###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#DifficultyLevel
so:DifficultyLevel rdf:type owl:Class ;
    rdfs:comment "Qualitative difficulty level used to categorize technical complexity profiles (e.g., easy, intermediate, difficult)."@en ;
    rdfs:label "Difficulty level"@en .

###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasDifficultyLevel
so:hasDifficultyLevel rdf:type owl:ObjectProperty ;
    rdfs:domain so:TechnicalComplexityProfile ;
    rdfs:range  so:DifficultyLevel ;
    rdfs:comment "Associates a technical complexity profile (local or global) with a qualitative difficulty level."@en ;
    rdfs:label "has difficulty level"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#EasyDifficulty
so:EasyDifficulty rdf:type owl:Class ;
    rdfs:subClassOf so:DifficultyLevel ;
    rdfs:comment "Qualitative difficulty level: easy."@en ;
    rdfs:label "Easy difficulty"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#IntermediateDifficulty
so:IntermediateDifficulty rdf:type owl:Class ;
    rdfs:subClassOf so:DifficultyLevel ;
    rdfs:comment "Qualitative difficulty level: intermediate."@en ;
    rdfs:label "Intermediate difficulty"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#DifficultDifficulty
so:DifficultDifficulty rdf:type owl:Class ;
    rdfs:subClassOf so:DifficultyLevel ;
    rdfs:comment "Qualitative difficulty level: difficult or advanced."@en ;
    rdfs:label "Difficult difficulty"@en .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( so:EasyDifficulty
                so:IntermediateDifficulty
                so:DifficultDifficulty
              )
] .



###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasDuration
so:hasDuration rdf:type owl:ObjectProperty ;
               rdfs:domain ho:SymbolicEvent ;
               rdfs:range so:Duration ;
               rdfs:comment "Associates a note/rest with a conventional duration (whole, half, quarter, etc.)."@en ;
               rdfs:label "has duration"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasDynamic
so:hasDynamic rdf:type owl:ObjectProperty ;
              owl:inverseOf so:isDynamicOf ;
              rdfs:domain ho:SymbolicEvent ;
              rdfs:range mso:Dynamic ;
              rdfs:comment "Dynamic marking attached to the symbolic event (e.g., ff, p, crescendo)."@en ;
              rdfs:label "has dynamic"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasInstrument
so:hasInstrument rdf:type owl:ObjectProperty ;
                 rdfs:domain mo:MusicalWork ;
                 rdfs:range mo:Instrument ;
                 rdfs:comment "Relates a musical work to its intended instrument or instrumentation."@en ;
                 rdfs:label "has instrument"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasKey
so:hasKey rdf:type owl:ObjectProperty ;
          owl:inverseOf so:isKeyOf ;
          rdfs:domain mo:MusicalWork ;
          rdfs:range mto:Key ;
          rdfs:comment "Associates the work to its theoretical key (tonality)."@en ;
          rdfs:label "has key"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasKeySignature
so:hasKeySignature rdf:type owl:ObjectProperty ;
                   owl:inverseOf so:isKeySignatureOfMeasure ;
                   rdfs:domain mso:Measure ;
                   rdfs:range mto:KeySignature ;
                   rdfs:comment "Associates a measure with its notated key signature."@en ;
                   rdfs:label "has key signature"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasMeasure
so:hasMeasure rdf:type owl:ObjectProperty ;
              owl:inverseOf so:isMeasureOfPhrase ;
              rdfs:domain so:Phrase ;
              rdfs:range mso:Measure ;
              rdfs:comment "Associates a phrase to its measures."@en ;
              rdfs:label "has measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasMelodicNote
so:hasMelodicNote rdf:type owl:ObjectProperty ;
                  owl:inverseOf so:isMelodicNoteOf ;
                  rdfs:domain so:MelodicProgression ;
                  rdfs:range mso:Note ;
                  rdfs:comment "Note that belongs to the melodic progression (voice/line)."@en ;
                  rdfs:label "has melodic note"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasMovement
so:hasMovement rdf:type owl:ObjectProperty ;
               owl:inverseOf so:isMovementOf ;
               rdfs:domain mo:MusicalWork ;
               rdfs:range mso:Movement ;
               rdfs:comment "Associates a musical work with one of its movements."@en ;
               rdfs:label "has movement"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasNoteInChord
so:hasNoteInChord rdf:type owl:ObjectProperty ;
                  rdfs:domain co:Chord ;
                  rdfs:range mso:Note ;
                  rdfs:comment "Constituent note of the chord."@en ;
                  rdfs:label "has note (chord)"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasPhrase
so:hasPhrase rdf:type owl:ObjectProperty ;
             owl:inverseOf so:isPhraseOfSection ;
             rdfs:domain so:Section ;
             rdfs:range so:Phrase ;
             rdfs:comment "Associates a section with its phrases."@en ;
             rdfs:label "has phrase"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasPitch
so:hasPitch rdf:type owl:ObjectProperty ;
            rdfs:domain mso:Note ;
            rdfs:range so:Pitch ;
            rdfs:comment "Links a symbolic note to a diatonic pitch name (A–G)."@en ;
            rdfs:label "has pitch name"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasRomanNumeral
so:hasRomanNumeral rdf:type owl:ObjectProperty ;
                   owl:inverseOf so:isRomanNumeralOfChord ;
                   rdfs:domain co:Chord ;
                   rdfs:range so:RomanNumeral ;
                   rdfs:comment "Functional-harmony Roman numeral assigned to this chord."@en ;
                   rdfs:label "has Roman numeral"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasSection
so:hasSection rdf:type owl:ObjectProperty ;
              owl:inverseOf so:isSectionOfMovement ;
              rdfs:domain mso:Movement ;
              rdfs:range so:Section ;
              rdfs:comment "Associates a movement to one of its formal sections."@en ;
              rdfs:label "has section"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasSonataSection
so:hasSonataSection rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf so:hasSection ;
                    rdfs:domain so:SonataMovement ;
                    rdfs:range so:SonataSection ;
                    rdfs:comment "Associates a sonata movement with one of its sonata-form sections."@en ;
                    rdfs:label "has sonata section"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasSymbolicEvent
so:hasSymbolicEvent rdf:type owl:ObjectProperty ;
                    owl:inverseOf so:isInMeasure ;
                    rdfs:domain mso:Measure ;
                    rdfs:range ho:SymbolicEvent ;
                    rdfs:comment "Notes or rests contained in the measure."@en ;
                    rdfs:label "has symbolic event"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasTechnicalComplexityProfile
so:hasTechnicalComplexityProfile rdf:type owl:ObjectProperty ;
                                 rdfs:domain so:SonataMovement ;
                                 rdfs:range so:TechnicalComplexityProfile ;
                                 rdfs:comment "Links a sonata movement to an aggregated technical complexity profile computed from its measures."@en ;
                                 rdfs:label "has technical complexity profile"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasTempo
so:hasTempo rdf:type owl:ObjectProperty ;
            owl:inverseOf so:isTempoOf ;
            rdfs:domain mso:Measure ;
            rdfs:range so:Tempo ;
            rdfs:comment "Associates a measure to its tempo."@en ;
            rdfs:label "has tempo"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasTimeSignature
so:hasTimeSignature rdf:type owl:ObjectProperty ;
                    owl:inverseOf so:timeSignatureOf ;
                    rdfs:domain mso:Measure ;
                    rdfs:range mto:TimeSignature ;
                    rdfs:comment "Relates a measure to its notated time signature."@en ;
                    rdfs:label "has time signature"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#hasTonality
so:hasTonality rdf:type owl:ObjectProperty ;
               rdfs:domain mso:Measure ;
               rdfs:range mto:Key ;
               rdfs:comment "For a concrete measure: the theoretical key (tonality) in effect."@en ;
               rdfs:label "has tonality"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isArticulationOf
so:isArticulationOf rdf:type owl:ObjectProperty ;
                    rdfs:domain mso:Articulation ;
                    rdfs:range ho:SymbolicEvent ;
                    rdfs:comment "Symbolic event to which this articulation applies."@en ;
                    rdfs:label "is articulation of"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isDynamicOf
so:isDynamicOf rdf:type owl:ObjectProperty ;
               rdfs:domain mso:Dynamic ;
               rdfs:range ho:SymbolicEvent ;
               rdfs:comment "Symbolic event to which this dynamic marking applies."@en ;
               rdfs:label "is dynamic of"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isInMeasure
so:isInMeasure rdf:type owl:ObjectProperty ;
               rdfs:domain ho:SymbolicEvent ;
               rdfs:range mso:Measure ;
               rdfs:comment "Links a symbolic event to the measure that contains it."@en ;
               rdfs:label "is in measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isKeyOf
so:isKeyOf rdf:type owl:ObjectProperty ;
           rdfs:domain mto:Key ;
           rdfs:range mo:MusicalWork ;
           rdfs:label "is key of work"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isKeySignatureOfMeasure
so:isKeySignatureOfMeasure rdf:type owl:ObjectProperty ;
                           rdfs:domain mto:KeySignature ;
                           rdfs:range mso:Measure ;
                           rdfs:comment "Inverse relation: the measure to which a key signature applies."@en ;
                           rdfs:label "is key signature of measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isMeasureOfPhrase
so:isMeasureOfPhrase rdf:type owl:ObjectProperty ;
                     rdfs:domain mso:Measure ;
                     rdfs:range so:Phrase ;
                     rdfs:label "is measure of phrase"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isMeasureOfStaff
so:isMeasureOfStaff rdf:type owl:ObjectProperty ;
                    owl:inverseOf so:staffHasMeasure ;
                    rdfs:domain mso:Measure ;
                    rdfs:range mso:Staff ;
                    rdfs:comment "Inverse relation of staffHasMeasure: links a measure to the staves it belongs to."@en ;
                    rdfs:label "is measure of staff"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isMelodicNoteOf
so:isMelodicNoteOf rdf:type owl:ObjectProperty ;
                   rdfs:domain mso:Note ;
                   rdfs:range so:MelodicProgression ;
                   rdfs:comment "Melodic progression (voice/line) to which the note belongs."@en ;
                   rdfs:label "is melodic note of"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isMovementOf
so:isMovementOf rdf:type owl:ObjectProperty ;
                rdfs:domain mso:Movement ;
                rdfs:range mo:MusicalWork ;
                rdfs:label "is movement of work"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isPhraseOfSection
so:isPhraseOfSection rdf:type owl:ObjectProperty ;
                     rdfs:domain so:Phrase ;
                     rdfs:range so:Section ;
                     rdfs:label "is phrase of section"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isRomanNumeralOfChord
so:isRomanNumeralOfChord rdf:type owl:ObjectProperty ;
                         rdfs:domain so:RomanNumeral ;
                         rdfs:range co:Chord ;
                         rdfs:comment "Inverse link: chord described by this Roman numeral analysis."@en ;
                         rdfs:label "is Roman numeral of chord"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isSectionOfMovement
so:isSectionOfMovement rdf:type owl:ObjectProperty ;
                       rdfs:domain so:Section ;
                       rdfs:range mso:Movement ;
                       rdfs:label "is section of movement"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#isTempoOf
so:isTempoOf rdf:type owl:ObjectProperty ;
             rdfs:domain so:Tempo ;
             rdfs:range mso:Measure ;
             rdfs:comment "Associates a tempo to its measure."@en ;
             rdfs:label "tempo of"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#movementHasChord
so:movementHasChord rdf:type owl:ObjectProperty ;
                    rdfs:domain mso:Movement ;
                    rdfs:range co:Chord ;
                    owl:propertyChainAxiom ( so:hasSection
                                             so:hasPhrase
                                             so:hasMeasure
                                             so:hasChordInMeasure
                                           ) ;
                    rdfs:label "movement has chord"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#movementHasMeasure
so:movementHasMeasure rdf:type owl:ObjectProperty ;
                      rdfs:domain mso:Movement ;
                      rdfs:range mso:Measure ;
                      owl:propertyChainAxiom ( so:hasSection
                                               so:hasPhrase
                                               so:hasMeasure
                                             ) ;
                      rdfs:label "movement has measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#movementHasStaff
so:movementHasStaff rdf:type owl:ObjectProperty ;
                    rdfs:domain mso:Movement ;
                    rdfs:range mso:Staff ;
                    rdfs:comment "Relates a movement to the staves used in its score ."@en ;
                    rdfs:label "movement has staff"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#movementHasSymbolicEvent
so:movementHasSymbolicEvent rdf:type owl:ObjectProperty ;
                            rdfs:domain mso:Movement ;
                            rdfs:range ho:SymbolicEvent ;
                            owl:propertyChainAxiom ( so:hasSection
                                                     so:hasPhrase
                                                     so:hasMeasure
                                                     so:hasSymbolicEvent
                                                   ) ;
                            rdfs:label "movement has symbolic event"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#phraseHasSymbolicEvent
so:phraseHasSymbolicEvent rdf:type owl:ObjectProperty ;
                          rdfs:domain so:Phrase ;
                          rdfs:range ho:SymbolicEvent ;
                          owl:propertyChainAxiom ( so:hasMeasure
                                                   so:hasSymbolicEvent
                                                 ) ;
                          rdfs:label "phrase has symbolic event"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#relativeMajor
so:relativeMajor rdf:type owl:ObjectProperty ;
                 owl:inverseOf so:relativeMinor .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#representsKey
so:representsKey rdf:type owl:ObjectProperty ;
                 rdfs:domain mto:KeySignature ;
                 rdfs:range mto:Key ;
                 rdfs:comment "Between individuals: a key signature instance typically represents one or more keys (e.g., G major and E minor for one sharp)."@en ;
                 rdfs:label "represents key"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#rnKey
so:rnKey rdf:type owl:ObjectProperty ;
         rdfs:domain so:RomanNumeral ;
         rdfs:range mto:Key ;
         rdfs:comment "Key context for the Roman numeral (e.g., the prevailing tonality of the passage)."@en ;
         rdfs:label "key"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#sectionHasChord
so:sectionHasChord rdf:type owl:ObjectProperty ;
                   rdfs:domain so:Section ;
                   rdfs:range co:Chord ;
                   owl:propertyChainAxiom ( so:hasPhrase
                                            so:hasMeasure
                                            so:hasChordInMeasure
                                          ) ;
                   rdfs:label "section has chord"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#sectionHasMeasure
so:sectionHasMeasure rdf:type owl:ObjectProperty ;
                     rdfs:domain so:Section ;
                     rdfs:range mso:Measure ;
                     owl:propertyChainAxiom ( so:hasPhrase
                                              so:hasMeasure
                                            ) ;
                     rdfs:label "section has measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#sectionHasSymbolicEvent
so:sectionHasSymbolicEvent rdf:type owl:ObjectProperty ;
                           rdfs:domain so:Section ;
                           rdfs:range ho:SymbolicEvent ;
                           owl:propertyChainAxiom ( so:hasPhrase
                                                    so:hasMeasure
                                                    so:hasSymbolicEvent
                                                  ) ;
                           rdfs:label "section has symbolic event"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#sonataMovementHasPianoStaff
so:sonataMovementHasPianoStaff rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf so:movementHasStaff ;
                               rdfs:domain so:SonataMovement ;
                               rdfs:range so:PianoStaff ;
                               rdfs:comment "Relates a sonata movement to the piano staves (upper and lower) of its grand staff."@en ;
                               rdfs:label "sonata movement has piano staff"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#staffHasClef
so:staffHasClef rdf:type owl:ObjectProperty ;
                rdfs:domain mso:Staff ;
                rdfs:range mso:Clef ;
                rdfs:comment "Relates a staff to the clefs that define its reference pitch over time."@en ;
                rdfs:label "staff has clef"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#staffHasMeasure
so:staffHasMeasure rdf:type owl:ObjectProperty ;
                   rdfs:domain mso:Staff ;
                   rdfs:range mso:Measure ;
                   rdfs:comment "Relates a staff to the measures that are notated on it."@en ;
                   rdfs:label "staff has measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#startMeasure
so:startMeasure rdf:type owl:ObjectProperty ;
                rdfs:domain so:StructuralElement ;
                rdfs:range mso:Measure ;
                rdfs:comment "First measure covered by the structural element (movement, section, phrase)."@en ;
                rdfs:label "start measure"@en .


###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#timeSignatureOf
so:timeSignatureOf rdf:type owl:ObjectProperty ;
                   rdfs:domain mto:TimeSignature ;
                   rdfs:range mso:Measure ;
