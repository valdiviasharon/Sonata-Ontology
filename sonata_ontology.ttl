@prefix so: <https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#> .  
## @base <https://example.org/sonata-ontology> .
## @prefix so: <https://example.org/sonata-ontology#> .

# HaMSE Ontology
@prefix ho:  <https://github.com/andreamust/HaMSE_Ontology/schema#> .
# Music Ontology
@prefix mo: <http://purl.org/ontology/mo/> .
# Music Theory Ontology
@prefix mto: <http://purl.org/ontology/mto/> .
# MusicOWL
@prefix mso: <http://linkeddata.uni-muenster.de/ontology/musicscore#> .
# OMRAS2 Chord Ontology
@prefix co: <http://purl.org/ontology/chord/> .
# Music Note Ontology
@prefix mno: <https://purl.org/andreapoltronieri/notepattern#> .

### ===== Additional Ontologies =====
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix vann:  <http://purl.org/vocab/vann/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .


### HEADER

<https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#>
  a owl:Ontology ;
  dct:title "Sonata Ontology"@en ;
  dct:description "An ontology focused on the analysis, representation, and preservation of piano sonatas."@en ;
  dct:creator "Sharon Valdivia" ;
  owl:versionInfo "0.1.0" ;
  vann:preferredNamespacePrefix "so" ;
  vann:preferredNamespaceUri "https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#" .  


#################################################################
## METADATA 
#################################################################

so:Sonata a owl:Class ;
  rdfs:subClassOf mo:MusicalWork ;
  rdfs:label "Sonata"@en ;
  rdfs:comment "A musical work in sonata form; specialization of mo:MusicalWork."@en .

so:title a owl:DatatypeProperty ;
  rdfs:domain mo:MusicalWork ; rdfs:range xsd:string ;
  rdfs:label "title"@en ;
  rdfs:comment "Title of the musical work (composition)."@en .

so:composer a owl:DatatypeProperty ;
  rdfs:domain mo:MusicalWork ; rdfs:range xsd:string ;
  rdfs:label "composer"@en ;
  rdfs:comment "Composer's name of the musical work ."@en .

so:hasKey a owl:ObjectProperty ;
  rdfs:domain mo:MusicalWork ; rdfs:range  mto:Key ;
  rdfs:label  "has key"@en ;
  rdfs:comment "Associates the work to its theoretical key (tonality)."@en .

so:isKeyOfWork a owl:ObjectProperty ;
  rdfs:domain mto:Key ;
  rdfs:range  mo:MusicalWork ;
  rdfs:label  "is key of work"@en ;
  owl:inverseOf so:hasKey .

#################################################################
# STRUCTURE
#################################################################

mso:Movement a owl:Class;
  rdfs:label "Movement"@en ;
  rdfs:comment "A movement of a Musical Work"@en .

so:MovementNumber a owl:DatatypeProperty;
  rdfs:domain mso:Movement ; rdfs:range xsd:integer ;
  rdfs:label "MovementNumber"@en ;
  rdfs:comment "Movement number in the score."@en .

so:hasMovement a owl:ObjectProperty ;
  rdfs:domain mo:MusicalWork ; rdfs:range mso:Movement ;
  rdfs:label "has movement"@en ;
  rdfs:comment "Associates a musical work with one of its movements."@en .

so:isMovementOfWork a owl:ObjectProperty ;
  rdfs:domain mso:Movement ; rdfs:range mo:MusicalWork ;
  rdfs:label "is movement of work"@en ;
  owl:inverseOf so:hasMovement .


mno:Section a owl:Class ;
  rdfs:label "Section"@en ;
  rdfs:comment "A structural section of a movement"@en .

so:hasSection a owl:ObjectProperty ;
  rdfs:domain mso:Movement ; rdfs:range mno:Section ;
  rdfs:label "has section"@en ;
  rdfs:comment "Associates a movement to one of its formal sections."@en .

so:isSectionOfMovement a owl:ObjectProperty ;
  rdfs:domain mno:Section ; rdfs:range mso:Movement ;
  rdfs:label "is section of movement"@en ;
  owl:inverseOf so:hasSection .

  
so:SonataSection a owl:Class ;
  rdfs:subClassOf mno:Section ;
  rdfs:label "Sonata Section"@en ;
  rdfs:comment "A structural section of a sonata (e.g., Exposition, Development, Recapitulation)."@en .

so:Exposition a owl:Class ;
  rdfs:subClassOf so:SonataSection ;
  rdfs:label "Exposition"@en ;
  rdfs:comment "Sonata-form exposition."@en .

so:Development a owl:Class ;
  rdfs:subClassOf so:SonataSection ;
  rdfs:label "Development"@en ;
  rdfs:comment "Sonata-form development."@en .

so:Recapitulation a owl:Class ;
  rdfs:subClassOf so:SonataSection ;
  rdfs:label "Recapitulation"@en ;
  rdfs:comment "Sonata-form recapitulation."@en .

so:Phrase a owl:Class ;
  rdfs:label "Phrase"@en ;
  rdfs:comment "A short musical unit forming a coherent idea."@en .

so:hasPhrase a owl:ObjectProperty ;
  rdfs:domain mno:Section ; rdfs:range so:Phrase ;
  rdfs:label "has phrase"@en ;
  rdfs:comment "Associates a phrase to its section."@en .

so:isPhraseOfSection a owl:ObjectProperty ;
  rdfs:domain so:Phrase ; rdfs:range mno:Section ;
  rdfs:label "is phrase of section"@en ;
  owl:inverseOf so:hasPhrase .

so:hasMeasure a owl:ObjectProperty ;
  rdfs:domain so:Phrase ; rdfs:range mso:Measure ;
  rdfs:label "has measure"@en ;
  rdfs:comment "Associates a phrase to its measures."@en .

so:isMeasureOfPhrase a owl:ObjectProperty ;
  rdfs:domain mso:Measure ; rdfs:range so:Phrase ;
  rdfs:label "is measure of phrase"@en ;
  owl:inverseOf so:hasMeasure .

so:number a owl:DatatypeProperty ;
  rdfs:domain mso:Measure ; rdfs:range xsd:integer ;
  rdfs:label "number"@en ;
  rdfs:comment "Measure number in the score (1-based)."@en .

so:startMeasure a owl:ObjectProperty ;
  rdfs:domain so:Phrase ; rdfs:range mso:Measure ;
  rdfs:label "start measure"@en ;
  rdfs:comment "First measure covered by the phrase/section."@en .

so:endMeasure a owl:ObjectProperty ;
  rdfs:domain so:Phrase ; rdfs:range mso:Measure ;
  rdfs:label "end measure"@en ;
  rdfs:comment "Last measure covered by the phrase/section."@en .

so:hasSymbolicEvent a owl:ObjectProperty ;
  rdfs:domain mso:Measure ; rdfs:range ho:SymbolicEvent ;
  rdfs:label "has symbolic event"@en ;
  rdfs:comment "Notes or rests contained in the measure."@en ;
  owl:inverseOf so:measure .

#################################################################
## MUSIC NOTATION
#################################################################
# Previous class of another ontologies: 
#  mso:Measure, mso:Rest, mso:Clef, mso:TimeSignature, mso:Dynamic, mso:Articulation;
#  ho:SymbolicEvent

mso:Note a owl:Class;
  rdfs:subClassOf ho:SymbolicEvent ;
  rdfs:label "Note"@en;
  rdfs:comment "Symbolic Event of type NOTE"@en .

mso:Rest a owl:Class;
  rdfs:subClassOf ho:SymbolicEvent ;
  rdfs:label "Rest"@en;
  rdfs:comment "Symbolic Event of type REST"@en .

so:Accidental a owl:Class ;
  rdfs:label "Accidental"@en ;
  rdfs:comment "An alteration to a pitch, such as sharp, flat, or natural."@en .

so:octave a owl:DatatypeProperty ;
  rdfs:domain mso:Note ; rdfs:range xsd:integer ;
  rdfs:label "octave"@en ;
  rdfs:comment "Octave number of the note (e.g., 4 for A4)."@en .

so:Pitch a owl:Class ;
  rdfs:label "Pitch name"@en ;
  rdfs:comment "Diatonic pitch name (natural letter name) used as a controlled vocabulary for note naming."@en .

so:hasAccidental a owl:ObjectProperty ;
  rdfs:domain so:Pitch ;
  rdfs:range so:Accidental ;
  rdfs:label "has accidental"@en ;
  rdfs:comment "Associates a pitch name with an accidental (sharp, flat, or natural)."@en .

so:hasPitch a owl:ObjectProperty ;
  rdfs:domain mso:Note ;
  rdfs:range  so:Pitch ;
  rdfs:label  "has pitch name"@en ;
  rdfs:comment "Links a symbolic note to a diatonic pitch name (A–G)."@en .

so:Duration a owl:Class ;
  rdfs:label "Duration"@en ;
  rdfs:comment "Conventional duration type of an event (whole, half, quarter, etc.). Encodes relative length as a fraction of a whole note."@en .

so:hasDuration a owl:ObjectProperty ;
  rdfs:domain ho:SymbolicEvent ; # , mso:Rest ;
  rdfs:range  so:Duration ;
  rdfs:label  "has Duration"@en ;
  rdfs:comment "Associates a note/rest with a conventional Duration (whole, half, quarter...), complementary to exact beat durations."@en .

so:measure a owl:ObjectProperty ;
  rdfs:domain ho:SymbolicEvent ; rdfs:range mso:Measure ;
  rdfs:label "measure"@en ;
  rdfs:comment "The measure the note belongs to."@en .

so:clef a owl:ObjectProperty ;
  rdfs:domain ho:SymbolicEvent; rdfs:range mso:Clef ;
  rdfs:label "clef"@en ;
  rdfs:comment "The clef that belong to a symbolic event."@en .


## Clef (mso:Clef)
so:line a owl:DatatypeProperty ;
  rdfs:domain mso:Clef ; rdfs:range xsd:integer ;
  rdfs:label "line"@en ;
  rdfs:comment "Reference staff line index used by the clef (e.g., 2 for treble G line)."@en .

so:sign a owl:DatatypeProperty ;
  rdfs:domain mso:Clef ; rdfs:range xsd:string ;
  rdfs:label "sign"@en ;
  rdfs:comment "Clef sign (e.g., G, F, C)."@en .

## Time Signature (mso:TimeSignature)
so:numerator a owl:DatatypeProperty ;
  rdfs:domain mso:TimeSignature ; rdfs:range xsd:integer ;
  rdfs:label "numerator"@en ;
  rdfs:comment "Top number in the meter."@en .

so:denominator a owl:DatatypeProperty ;
  rdfs:domain mso:TimeSignature ; rdfs:range xsd:integer ;
  rdfs:label "denominator"@en ;
  rdfs:comment "Bottom number in the meter (beat unit)."@en .

so:symbol a owl:DatatypeProperty ;
  rdfs:domain mso:TimeSignature ; rdfs:range xsd:string ;
  rdfs:label "symbol"@en ;
  rdfs:comment "Optional symbolic form (e.g., 'common' C, 'cut' alla breve)."@en .

so:timeSignatureOf a owl:ObjectProperty ;
  rdfs:domain mso:TimeSignature ; rdfs:range mso:Measure ;
  rdfs:label "time signature of"@en ;
  rdfs:comment "Associates a time signature to its measure (if modeled per-measure)."@en .

so:hasTimeSignature a owl:ObjectProperty ;
  rdfs:domain mso:Measure ; rdfs:range mso:TimeSignature ;
  rdfs:label "has time signature"@en ;
  owl:inverseOf so:timeSignatureOf .

## Key & Key Signature (mto:Key + so:KeySignature)


so:KeySignature a owl:Class ;
  rdfs:label "Key Signature"@en ;
  rdfs:comment "Notated set of accidentals indicating the key for the staff."@en .

so:hasKeySignature a owl:ObjectProperty ;
  rdfs:domain mso:Measure ; rdfs:range so:KeySignature ;
  rdfs:label "has key signature"@en ;
  rdfs:comment "Associates a measure to its key signature."@en .

so:isKeySignatureOfMeasure a owl:ObjectProperty ;
  rdfs:domain so:KeySignature ; rdfs:range mso:Measure ;
  rdfs:label "is key signature of meaure"@en ;
  owl:inverseOf so:hasKeySignature .

so:fifths a owl:DatatypeProperty ;
  rdfs:domain so:KeySignature ; rdfs:range xsd:integer ;
  rdfs:label "fifths"@en ;
  rdfs:comment "Number of fifths (sharps positive, flats negative)."@en .

so:mode a owl:DatatypeProperty ;
  rdfs:domain so:KeySignature ; rdfs:range xsd:string ;
  rdfs:label "mode"@en ;
  rdfs:comment "Mode name (e.g., major, minor)."@en .

so:tonality a owl:ObjectProperty ;
  rdfs:domain so:KeySignature ; rdfs:range mto:Key ;
  rdfs:label "tonality"@en ;
  rdfs:comment "The theoretical key that the signature encodes."@en .

## Tempo (so:Tempo)
so:Tempo a owl:Class ;
  rdfs:label "Tempo Marking"@en ;
  rdfs:comment "Tempo indication as text (e.g., Allegro) and/or BPM (e.g., ♩=120)."@en .

so:bpm a owl:DatatypeProperty ;
  rdfs:domain so:Tempo ; rdfs:range xsd:decimal ;
  rdfs:label "BPM"@en ;
  rdfs:comment "Beats per minute (tempo)."@en .

so:tempoText a owl:DatatypeProperty ;
  rdfs:domain so:Tempo ; rdfs:range xsd:string ;
  rdfs:label "tempo text"@en ;
  rdfs:comment "Textual tempo indication (e.g., Allegro, Adagio)."@en .

so:tempoOf a owl:ObjectProperty ;
  rdfs:domain so:Tempo ; rdfs:range mso:Measure ;
  rdfs:label "tempo of"@en ;
  rdfs:comment "Associates a tempo to its measure."@en .

so:hasTempo a owl:ObjectProperty ;
  rdfs:domain mso:Measure ; rdfs:range so:Tempo ;
  rdfs:label "has tempo"@en ;
  rdfs:comment "Associates a measure to its tempo."@en ;
  owl:inverseOf so:tempoOf .

#################################################################
# HARMONY
#################################################################
# (co:Chord)

so:HarmonicProgression a owl:Class ;
  rdfs:label "Harmonic progression"@en ;
  rdfs:comment "A sequence of chords forming a harmonic path."@en .
  
so:hasChord a owl:ObjectProperty ;
  rdfs:domain so:HarmonicProgression ; rdfs:range co:Chord ;
  rdfs:label "has chord"@en ;
  rdfs:comment "Chord belonging to the progression."@en .

so:order a owl:DatatypeProperty ;
  rdfs:domain co:Chord ; rdfs:range xsd:integer ;
  rdfs:label "order"@en ;
  rdfs:comment "Optional index of the chord inside the progression (1-based)."@en .

## Chord (co:Chord) 
so:bass a owl:DatatypeProperty ;
  rdfs:domain co:Chord ; rdfs:range xsd:string ;
  rdfs:label "bass"@en ;
  rdfs:comment "Bass pitch or descriptor (e.g., E2)."@en .

so:figure a owl:DatatypeProperty ;
  rdfs:domain co:Chord ; rdfs:range xsd:string ;
  rdfs:label "figure"@en ;
  rdfs:comment "Figured-bass or analytical figure (e.g., 6-5)."@en .

so:commonName a owl:DatatypeProperty ;
  rdfs:domain co:Chord ; rdfs:range xsd:string ;
  rdfs:label "common name"@en ;
  rdfs:comment "Common chord name (e.g., Cmaj7, G7sus4)."@en .

so:pitchedCommonName a owl:DatatypeProperty ;
  rdfs:domain co:Chord ; rdfs:range xsd:string ;
  rdfs:label "pitched common name"@en ;
  rdfs:comment "Chord name including specific pitch spelling if needed."@en .

so:symbol a owl:DatatypeProperty ;
  rdfs:domain co:Chord ; rdfs:range xsd:string ;
  rdfs:label "symbol"@en ;
  rdfs:comment "Lead-sheet chord symbol as written."@en .

so:hasNoteInChord a owl:ObjectProperty ;
  rdfs:domain co:Chord ; rdfs:range mso:Note ;
  rdfs:label "has note (chord)"@en ;
  rdfs:comment "Constituent note of the chord."@en .

so:hasRomanNumeral a owl:ObjectProperty ;
  rdfs:domain co:Chord ; rdfs:range so:RomanNumeral ;
  rdfs:label "has Roman numeral"@en ;
  rdfs:comment "Functional-harmony Roman numeral assigned to this chord."@en .

so:isRomanNumeralOfChord a owl:ObjectProperty ;
  rdfs:domain so:RomanNumeral ; rdfs:range co:Chord ;
  rdfs:label "is Roman numeral of chord"@en ;
  owl:inverseOf so:hasRomanNumeral .

so:chordMeasure a owl:ObjectProperty ;
  rdfs:domain co:Chord ; rdfs:range mso:Measure ;
  rdfs:label "measure"@en ;
  rdfs:comment "Measure where the chord occurs."@en .

so:measureHasChord a owl:ObjectProperty ;
  rdfs:domain mso:Measure ; rdfs:range co:Chord ;
  rdfs:label "measure has chord"@en ;
  owl:inverseOf so:chordMeasure .

## Roman numeral (local)
so:RomanNumeral a owl:Class ;
  rdfs:label "Roman numeral chord"@en ;
  rdfs:comment "Chord expressed in Roman-numeral functional analysis."@en .

so:rnFigure a owl:DatatypeProperty ;
  rdfs:domain so:RomanNumeral ; rdfs:range xsd:string ;
  rdfs:label "figure"@en ;
  rdfs:comment "Roman numeral figure (e.g., V, ii°, Ger+6)."@en .

so:inversion a owl:DatatypeProperty ;
  rdfs:domain so:RomanNumeral ; rdfs:range xsd:string ;
  rdfs:label "inversion"@en ;
  rdfs:comment "Inversion indicator (e.g., root, 6, 6/4)."@en .

so:rnBass a owl:DatatypeProperty ;
  rdfs:domain so:RomanNumeral ; rdfs:range xsd:string ;
  rdfs:label "bass"@en ;
  rdfs:comment "Bass pitch or scale-degree for the Roman numeral."@en .

so:rnKey a owl:ObjectProperty ;
  rdfs:domain so:RomanNumeral ; rdfs:range mto:Key ;
  rdfs:label "key"@en ;
  rdfs:comment "Key context for the Roman numeral."@en .

so:rnMode a owl:DatatypeProperty ;
  rdfs:domain so:RomanNumeral ; rdfs:range xsd:string ;
  rdfs:label "mode"@en ;
  rdfs:comment "Mode for the Roman numeral context (e.g., major/minor)."@en .

#################################################################
# MELODY
#################################################################

so:MelodicProgression a owl:Class ;
  rdfs:label "Melodic progression"@en ;
  rdfs:comment "A sequence of notes forming a melodic line."@en .

so:hasMelodicNote a owl:ObjectProperty ;
  rdfs:domain so:MelodicProgression ; rdfs:range mso:Note ;
  rdfs:label "has note (melodic)"@en ;
  rdfs:comment "Note that belongs to the melodic progression."@en .

so:isMelodicNoteOf a owl:ObjectProperty ;
  rdfs:domain mso:Note ; rdfs:range so:MelodicProgression ;
  rdfs:label "is melodic note of"@en ;
  rdfs:comment "melodic progression of a note."@en ;
  owl:inverseOf so:hasMelodicNote .

#################################################################
# EXPRESSION
#################################################################
# (mso:Dynamic, mso:Articulation)

so:value a owl:DatatypeProperty ;
  rdfs:domain mso:Dynamic ; rdfs:range xsd:string ;
  rdfs:label "value"@en ;
  rdfs:comment "Dynamic value (e.g., p, mf, crescendo)."@en .

so:hasArticulation a owl:ObjectProperty ;
  rdfs:domain mso:Note ; rdfs:range mso:Articulation ;
  rdfs:label "has articulation"@en ;
  rdfs:comment "Articulation marking attached to the note (e.g., staccato, accent)."@en .

so:isArticulationOf a owl:ObjectProperty ;
  rdfs:domain mso:Articulation ; rdfs:range mso:Note ;
  rdfs:label "is articulation of"@en ;
  rdfs:comment "Articulation of a note (e.g., staccato, accent)."@en ;
  owl:inverseOf so:hasArticulation .

so:hasDynamic a owl:ObjectProperty ;
  rdfs:domain mso:Note ; rdfs:range mso:Dynamic ;
  rdfs:label "has dynamic"@en ;
  rdfs:comment "Dynamic attached to the note (e.g., ff, fff, p, pp, ppp)."@en .

so:isDynamicOf a owl:ObjectProperty ;
  rdfs:domain mso:Dynamic ; rdfs:range mso:Note ;
  rdfs:label "is dynamic of"@en ;
  rdfs:comment "Dynamic of a note (e.g., ff, fff, p, pp, ppp)."@en .

##### SUB CLASSES // INSTANCES  #####

# PITCHES
#################################################################
so:A a owl:Class ;
  rdfs:subClassOf so:Pitch ;
  rdfs:label "A"@en ;
  rdfs:comment "Diatonic pitch name A (natural)."@en .

so:B a owl:Class ;
  rdfs:subClassOf so:Pitch ;
  rdfs:label "B"@en ;
  rdfs:comment "Diatonic pitch name B (natural)."@en .

so:C a owl:Class ;
  rdfs:subClassOf so:Pitch ;
  rdfs:label "C"@en ;
  rdfs:comment "Diatonic pitch name C (natural)."@en .

so:D a owl:Class ;
  rdfs:subClassOf so:Pitch ;
  rdfs:label "D"@en ;
  rdfs:comment "Diatonic pitch name D (natural)."@en .

so:E a owl:Class ;
  rdfs:subClassOf so:Pitch ;
  rdfs:label "E"@en ;
  rdfs:comment "Diatonic pitch name E (natural)."@en .

so:F a owl:Class ;
  rdfs:subClassOf so:Pitch ;
  rdfs:label "F"@en ;
  rdfs:comment "Diatonic pitch name F (natural)."@en .

so:G a owl:Class ;
  rdfs:subClassOf so:Pitch ;
  rdfs:label "G"@en ;
  rdfs:comment "Diatonic pitch name G (natural)."@en .

#DURATIONS
#################################################################

so:WholeNote a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "whole note"@en ;
  rdfs:comment "Whole note (semibreve; 'redonda')."@en .

so:HalfNote a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "half note"@en ;
  rdfs:comment "Half note (minim; 'blanca')."@en .

so:QuarterNote a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "quarter note"@en ;
  rdfs:comment "Quarter note (crotchet; 'negra')."@en .

so:EighthNote a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "eighth note"@en ;
  rdfs:comment "Eighth note (quaver; 'corchea')."@en .

so:SixteenthNote a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "sixteenth note"@en ;
  rdfs:comment "Sixteenth note (semiquaver; 'semicorchea')."@en .

so:ThirtySecondNote a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "thirty-second note"@en ;
  rdfs:comment "Thirty-second note (demisemiquaver; 'fusa')."@en .

so:SixtyFourthNote a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "sixty-fourth note"@en ;
  rdfs:comment "Sixty-fourth note (hemidemisemiquaver; 'semifusa')."@en .

so:DottedHalf a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "dotted half"@en ;
  rdfs:comment "Dotted half note (adds 1/2 of the value)."@en .

so:DottedQuarter a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "dotted quarter"@en ;
  rdfs:comment "Dotted quarter note (adds 1/2 of the value)."@en .

so:DottedEighth a owl:Class ;
  rdfs:subClassOf so:Duration ;
  rdfs:label "dotted eighth"@en ;
  rdfs:comment "Dotted eighth note (adds 1/2 of the value)."@en .


# ACCIDENTALS (ALTERATIONS)
#################################################################

so:Sharp a owl:Class ;
  rdfs:subClassOf so:Accidental ;
  rdfs:label "Sharp"@en ;
  rdfs:comment "Sharp accidental, raises a pitch by a half-step."@en .

so:Flat a owl:Class ;
  rdfs:subClassOf so:Accidental ;
  rdfs:label "Flat"@en ;
  rdfs:comment "Flat accidental, lowers a pitch by a half-step."@en .

so:Natural a owl:Class ;
  rdfs:subClassOf so:Accidental ;
  rdfs:label "Natural"@en ;
  rdfs:comment "Natural accidental, cancels a previous sharp or flat."@en .

# CLEFS
#################################################################

so:TrebleClef a owl:Class ;
  rdfs:subClassOf mso:Clef ;
  rdfs:label "Treble clef (G clef)"@en ;
  rdfs:comment "G clef on line 2; typical right-hand staff in piano."@en .

so:BassClef a owl:Class ;
  rdfs:subClassOf mso:Clef ;
  rdfs:label "Bass clef (F clef)"@en ;
  rdfs:comment "F clef on line 4; typical left-hand staff in piano."@en .

so:AltoClef a owl:Class ;
  rdfs:subClassOf mso:Clef ;
  rdfs:label "Alto clef (C clef)"@en ;
  rdfs:comment "C clef on line 3; centers C4 on line 3."@en .

so:TenorClef a owl:Class ;
  rdfs:subClassOf mso:Clef ;
  rdfs:label "Tenor clef (C clef)"@en ;
  rdfs:comment "C clef on line 4; centers C4 on line 4."@en .

# TIME SIGNATURE
#################################################################

so:TS_4_4 a owl:Class ;
  rdfs:subClassOf mso:TimeSignature ;
