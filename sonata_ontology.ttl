###  https://github.com/valdiviasharon/Sonata-Ontology/sonata_ontology#SonataMovement
so:SonataMovement rdf:type owl:Class ;
                  rdfs:subClassOf mso:Movement ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty so:movementHasStaff ;
                                    owl:someValuesFrom so:PianoStaff
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty so:hasSection ;
                                    owl:allValuesFrom so:SonataSection
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty so:hasSonataSection ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass so:Development
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty so:hasSonataSection ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass so:Exposition
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty so:hasSonataSection ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass so:Recapitulation
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty so:hasSonataSection ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass so:Coda
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty so:hasSonataSection ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass so:Introduction
                                  ] ;
                  rdfs:comment "A movement in sonata form, which has exactly one exposition, one development and one recapitulation, and may optionally include at most one introduction and/or one coda."@en ;
                  rdfs:label "Sonata Movement"@en .


